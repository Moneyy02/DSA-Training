class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
       ListNode* temp = head;
       int count =0;

       while(temp!= NULL){
        count++;
        temp = temp->next;
       }
       if(n==count){
        return head->next;
       }
       temp = head;
       int pos = count-n-1;
       for(int i=0;i<pos;i++){
        temp= temp->next;
       }
       temp->next= temp->next->next;
        return head;
    }
};
