class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
      ListNode* dummy = new ListNode(0);
      dummy->next = head;
      ListNode* p= dummy;
      ListNode* q = dummy;

      for(int i=0;i<=n;i++){
        p = p->next;
      }
      while(p!= NULL){
        q = q->next;
        p=p->next;
      }
      q->next = q->next->next;
      return dummy->next;
    }
};
